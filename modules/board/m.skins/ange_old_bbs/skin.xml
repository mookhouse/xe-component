<?xml version="1.0" encoding="UTF-8"?>
<skin version="0.2">
    <title xml:lang="ko">(구)앙쥬 게시판</title>
    <description xml:lang="ko">컨텐츠, QNA</description>
    <version>1.0</version>
    <date>2022-03-10</date>
    <author email_address="root@singleview.co.kr" link="http://singleview.co.kr">
        <name xml:lang="ko">singleview.co.kr</name>
    </author>
    <license>LGPL v2</license>
	<extra_vars>
        <group>
		<title xml:lang="ko">전체 설정</title>
			<var name="default_style" type="radio">
				<title xml:lang="ko">목록 유형</title>
				<options value="list">
					<title xml:lang="ko">목록형</title>
				</options>
				<options value="gallery">
					<title xml:lang="ko">갤러리형</title>
				</options>
				<options value="event">
					<title xml:lang="ko">이벤트형</title>
				</options>
				<options value="magazine">
					<title xml:lang="ko">잡지형</title>
				</options>
			</var>
		</group>
		<group>
			<title xml:lang="ko">추천글 위젯 설정</title>
			<var name="bbs_module_srl" type="text">
				<title xml:lang="ko">추천글 위젯이 추출할 게시판 srl</title>
			</var>
			<var name="curation_cookie_use" type="radio">
				<title xml:lang="ko">큐레이션 쿠키 사용</title>
				<description xml:lang="ko">출산예정일, 임신준비 큐레이션 사용</description>
				<options value="Y">
					<title xml:lang="ko">사용</title>
				</options>
				<options value="N">
					<title xml:lang="ko">미사용</title>
				</options>
			</var>
		</group>
    </extra_vars>
</skin>
