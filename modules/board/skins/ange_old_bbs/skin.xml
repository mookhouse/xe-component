<?xml version="1.0" encoding="UTF-8"?>
<skin version="0.2">
    <title xml:lang="ko">(구)앙쥬 게시판</title>
    <description xml:lang="ko">컨텐츠, QNA</description>
    <version>1.0</version>
    <date>2022-03-10</date>
    <author email_address="root@singleview.co.kr" link="http://singleview.co.kr">
        <name xml:lang="ko">singleview.co.kr</name>
    </author>
    <license>LGPL v2</license>
    <extra_vars>
        <group>
		<title xml:lang="ko">전체 설정</title>
			<var name="default_style" type="radio">
				<title xml:lang="ko">목록 유형</title>
				<options value="list">
					<title xml:lang="ko">목록형</title>
				</options>
				<options value="gallery">
					<title xml:lang="ko">갤러리형</title>
				</options>
				<options value="event">
					<title xml:lang="ko">이벤트형</title>
				</options>
				<options value="magazine">
					<title xml:lang="ko">잡지형</title>
				</options>
			</var>
			<var name="side_nav_bar" type="radio">
				<title xml:lang="ko">좌측 메뉴 연결 선택</title>
				<description xml:lang="ko">/layouts/old_ange/에서 설정한 메뉴 변수입니다.</description>
				<options value="CONTENTS_NAV">
					<title xml:lang="ko">CONTENTS_NAV 메뉴</title>
				</options>
				<options value="COMMUNITY_NAV">
					<title xml:lang="ko">COMMUNITY_NAV 메뉴</title>
				</options>
			</var>
		</group>
		<group>
		<title xml:lang="ko">예제 설정1</title>
			<var name="sub_title" type="text">
				<title xml:lang="ko">게시판 부제목</title>
			</var>
			<var name="title_img" type="image">
				<title xml:lang="ko">게시판 제목에 이미지 사용</title>
				<description xml:lang="ko">게시판 제목을 이 항목에 등록한 이미지로 사용할 수 있습니다.</description>
			</var>
		</group>
    </extra_vars>
</skin>