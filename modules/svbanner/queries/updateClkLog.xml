<query id="updateClkLog" action="update">
	<tables>
		<table name="svbanner_impression" />
	</tables>
	<columns>
		<column name="is_clicked" var="is_clicked" notnull="notnull" />
        <column name="clickdate" var="clickdate" default="curdate()" />
	</columns>
	<conditions>
		<condition operation="equal" column="imp_srl" var="imp_srl" notnull="notnull" />
	</conditions>
</query>