<table name="svbanner_client">
	<column name="client_srl" type="number" size="11" notnull="notnull" primary_key="primary_key" auto_increment="auto_increment"/>
	<!-- 광고주 생성자 srl -->
	<column name="member_srl" type="number" size="11" notnull="notnull" />
	<column name="client_name" type="varchar" size="160" notnull="notnull" unique="unique_client_name"/>
	<column name="is_active" type="char" size="1" notnull="notnull" default="Y" index="idx_active"/>
	<column name="history" type="text" />
	<column name="regdate" type="date" notnull="notnull" />
</table>