<filter name="insert" module="angemombox" act="procAngemomboxRegistration">
  <form>
    <node target="reason" required="true" maxlength="1000" />
  </form>
  <response callback_func="completeDocumentInserted">
    <tag name="error" />
    <tag name="message" />
    <tag name="mid" />
    <tag name="document_srl" />
  </response>
</filter>