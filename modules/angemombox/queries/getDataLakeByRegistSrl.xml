<query id="getDataLakeByRegistSrl" action="select">
    <tables>
        <table name="angemombox_data_lake" />
    </tables>
    <columns>
        <column name="doc_srl" />
        <!-- <column name="module_srl" /> -->
        <column name="baby_birth_name" />
        <column name="baby_gender"  />
        <column name="baby_birthday_yyyymmdd" />
		<column name="email_push" />
		<column name="sms_push" />
		<column name="post_push" />
		<column name="sponsor_push" />
    </columns>
    <conditions>
		<condition operation="equal" column="angeclub_registration_log_srl" var="angeclub_registration_log_srl" filter="number" notnull="notnull" />
    </conditions>
</query>