<query id="getBabyList" action="select">
    <tables>
        <table name="angemombox_baby_list" />
    </tables>
    <columns>
        <column name="name" />
        <column name="gender" />
        <column name="birthday" />
    </columns>
    <conditions>
		<condition operation="equal" column="member_srl" var="member_srl" filter="number" notnull="notnull" />
    </conditions>
</query>