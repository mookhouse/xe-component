<query id="updateDataLake" action="update" priority="LOW">
    <tables>
        <table name="angemombox_data_lake" />
    </tables>
    <columns>
        <column name="email_push" var="email_push" />
        <column name="sms_push" var="sms_push" />
        <column name="post_push" var="post_push" />
        <column name="sponsor_push" var="sponsor_push" />
    </columns>
    <conditions>
		<condition operation="equal" column="doc_srl" var="datalake_doc_srl" filter="number" />
    </conditions>
</query>