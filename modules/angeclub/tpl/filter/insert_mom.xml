<filter name="insert_mom" module="angeclub" act="procAngeclubAddMom">
  <form>
    <node target="reason" required="true" maxlength="1000" />
	  <node target="privacy_collection" required="true" />
	  <node target="privacy_sharing" required="true" />
  </form>
  <response callback_func="completeMomInserted">
    <tag name="error" />
    <tag name="message" />
    <tag name="mid" />
    <tag name="document_srl" />
  </response>
</filter>